{
  "name": "vox/datatables",
  "description": "A PHP wrapper for DataTables (http://www.datatables.net) plugin for jQuery.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "datatables",
    "grid",
    "wrapper"
  ],
  "homepage": "https://github.com/vox-tecnologia/datatables",
  "authors": [
    {
      "name": "Ítalo Lelis de Vietro",
      "email": "italolelis@gmail.com"
    }
  ],
  "require": {
    "doctrine/collections": "~1.6",
    "symfony/property-access": "~3.4",
    "symfony/event-dispatcher": "~3.4"
  },
  "require-dev": {
    "php": ">=5.4.0",
    "phpunit/phpunit": "~4.5",
    "symfony/var-dumper": "~3.4"
  },
  "autoload": {
    "psr-4": {
      "Datatable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Datatable\\": "tests/"
    }
  }
}
